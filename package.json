{
  "name": "tr3butor-rest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "doc-cli": "yarn next-swagger-doc-cli next-swagger-doc.json",
    "postbuild": "cpy 'models/**/*.swagger.yaml' public/openapi --flat",
    "openapi:yaml": "yarn typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'models/openapi' 'models/*.ts'"
  },
  "dependencies": {
    "@prisma/client": "4.4.0",
    "next": "12.3.1",
    "next-swagger-doc": "0.3.6",
    "next-transpile-modules": "^9.1.0",
    "prisma-schema": "git+https://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:x-oauth-basic@github.com/tr3butor-org/prisma-schema.git",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swagger-ui-react": "4.14.3"
  },
  "devDependencies": {
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-react": "^4.11.0",
    "cpy-cli": "4.2.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "prisma": "4.4.0",
    "typeconv": "1.8.0",
    "typescript": "4.8.4"
  },
  "prisma": {
    "schema": "node_modules/prisma-schema/schema.prisma"
  }
}
