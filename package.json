{
  "name": "intropia-rest",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postbuild": "cpy 'models/**/*.swagger.yaml' public/openapi --flat",
    "openapi:yaml": "yarn typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'models/openapi' 'models/*.ts'",
    "buildDoc": "yarn next-swagger-doc-cli next-swagger-doc.json"
  },
  "dependencies": {
    "@intropia-io/prisma-schema": "1.0.4",
    "@prisma/client": "^4.5.0",
    "next": "12.3.1",
    "next-swagger-doc": "0.3.6",
    "next-transpile-modules": "^9.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.55.0",
    "swagger-ui-react": "4.14.3"
  },
  "devDependencies": {
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-react": "^4.11.0",
    "cpy-cli": "4.2.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "prisma": "^4.5.0",
    "typeconv": "1.8.0",
    "typescript": "4.8.4"
  },
  "prisma": {
    "schema": "node_modules/@intropia-io/prisma-schema/schema.prisma"
  }
}
